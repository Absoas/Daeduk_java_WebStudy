<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.alba.dao.IOtherDAO">
	
	<select id="selectLicense" resultType="HashMap">
		select * from simplecode where codename like 'L%'
	</select>
	
	
	<select id="selectGrade" resultType="HashMap">
		select * from simplecode where codename like 'G%'
	</select>
</mapper>